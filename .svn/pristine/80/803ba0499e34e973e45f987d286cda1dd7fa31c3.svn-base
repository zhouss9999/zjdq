package wy.qingdao_atmosphere.datacenter.domain;

import javax.persistence.Entity;

/**
 * 字段信息管理
 * @author User
 *
 */
@Entity
public class Tableinfo {
	 private int fieldid; 		//字段信息ID
	 private String fieldname; 		//字段名称
	 private String fieldshowname; 	//字段显示名称
	 private int tableid; 			//表信息ID
	 private String fielddatatype; 	//字段数据类型
	 private String isdictionaries; //是否带字典
	 private String dicname; 		//字典名称
	 private int dicid; 			//字典ID
	 private int isused; 			//是否删除
	 private int isinstalled; 		//字段是否为内置字段,value=1是内置字段
	 private int orderintable; 		//字段在表里的顺序
	public int getFieldid() {
		return fieldid;
	}
	public void setFieldid(int fieldid) {
		this.fieldid = fieldid;
	}
	public String getFieldname() {
		return fieldname;
	}
	public void setFieldname(String fieldname) {
		this.fieldname = fieldname;
	}
	public String getFieldshowname() {
		return fieldshowname;
	}
	public void setFieldshowname(String fieldshowname) {
		this.fieldshowname = fieldshowname;
	}
	public int getTableid() {
		return tableid;
	}
	public void setTableid(int tableid) {
		this.tableid = tableid;
	}
	public String getFielddatatype() {
		return fielddatatype;
	}
	public void setFielddatatype(String fielddatatype) {
		this.fielddatatype = fielddatatype;
	}
	public String getIsdictionaries() {
		return isdictionaries;
	}
	public void setIsdictionaries(String isdictionaries) {
		this.isdictionaries = isdictionaries;
	}
	public String getDicname() {
		return dicname;
	}
	public void setDicname(String dicname) {
		this.dicname = dicname;
	}
	public int getDicid() {
		return dicid;
	}
	public void setDicid(int dicid) {
		this.dicid = dicid;
	}
	public int getIsused() {
		return isused;
	}
	public void setIsused(int isused) {
		this.isused = isused;
	}
	public int getIsinstalled() {
		return isinstalled;
	}
	public void setIsinstalled(int isinstalled) {
		this.isinstalled = isinstalled;
	}
	public int getOrderintable() {
		return orderintable;
	}
	public void setOrderintable(int orderintable) {
		this.orderintable = orderintable;
	}
}
